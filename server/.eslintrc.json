{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error", { "allow": ["error", "info", "warn"] }],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
